#custom-date {
  color: #ca365d;
}

#custom-power {
  color: #37489c;
  background-color: #db4b4b;
  border-radius: 5px;
  margin-right: 10px;
  margin-top: 5px;
  margin-bottom: 5px;
  margin-left: 0px;
  padding: 5px 10px;
}

#tray {
  background: #282828;
  margin: 5px 5px 5px 5px;
  border-radius: 16px;
  padding: 0px 5px;
  /*border-right: solid 1px #282738;*/
}

#clock {
  color: black;
  /* color: #ffff00; */
  /* background-color: #241717; */
  background-color: whitesmoke;
  padding-left: 12px;
  padding-right: 12px;
  margin-right: 4px;
  margin-left: 10px;
  margin-top: 4px;
  margin-bottom: 4px;
  /*border-left: solid 1px #282738;*/
}

#battery {
  color: black;
}

#battery.charging {
  color: #9ece6a;
}

#battery.warning:not(.charging) {
  background-color: #f7768e;
  color: black;
  border-radius: 5px 5px 5px 5px;
}

#backlight {
  border-radius: 0px 0px 0px 0px;
  color: black;
  margin: 5px;
  margin-left: 2px;
  margin-right: 14px;
  padding: 0px 0px;
}

#custom-redshift {
  color: black;
  margin-right: 15px;
}

#network {
  color: black;
  border-radius: 8px;
  margin-right: 5px;
}
#temperature {
  color: black;
}

#pulseaudio {
  color: black;
  border-radius: 8px;
  margin-left: 0px;
}

#pulseaudio.muted {
  background: transparent;
  color: black;
  border-radius: 8px;
  margin-left: 0px;
}

#custom-launcher {
  color: black;
  /* color: #a3f71d; */
  background-color: whitesmoke;
  /* background-color: #241717; */
  margin: 4px 10px 4px 4px;
  padding: 0px 19px 0px 14px;
  /* border-right: solid 1px #663e4a; */
  font-size: 13px;
}

#custom-launcher button:hover {
  background-color: red;
  color: transparent;
  border-radius: 8px;
  margin-right: -5px;
  margin-left: 10px;
}

#custom-playerctl {
  background: transparent;
  color: black;
  padding-left: 15px;
  padding-right: 15px;
  border-radius: 16px;
  margin-top: 5px;
  margin-bottom: 5px;
  margin-left: 5px;
  margin-right: 0px;
  font-size: 14px;
  border: 1px solid whitesmoke;
}

#custom-playerlabel {
  background: transparent;
  color: black;
  padding-left: 20px;
  padding-right: 15px;
  border-radius: 16px;
  /* border-left: solid 1px #282738; */
  /*border-right: solid 1px #282738;*/
  margin-top: 5px;
  margin-bottom: 5px;
  margin-left: -6px;
  font-weight: bold;
}

#window {
  background: #1f1c1c;
  background-color: #241717;
  padding-left: 15px;
  padding-right: 15px;
  border-radius: 16px;
  /*border-left: solid 1px #282738;*/
  /*border-right: solid 1px #282738;*/
  margin-top: 5px;
  margin-bottom: 5px;
}
