* {
  /*font-family: VictorMono, Iosevka Nerd Font, Noto Sans CJK;*/
  font-family:
    "RobotoMono Nerd Font",
    FontAwesome,
    Noto Sans CJK;
  font-size: 12px;
  padding: 0;
  margin: 0;
  font-weight: bold;
}

window#waybar {
  background-color: rgba(0, 0, 0, 0); /* 80% opaque black */

  /*border-radius:9px 9px 9px  9px;*/
  /*border-color: #00ff6a;*/
  /*border: 0.8px solid rgb(255, 255, 0);*/
}

#workspaces {
  background: rgba(0, 0, 0, 0);
  margin: 5px 5px 5px 10px;
  padding: 0px 5px 0px 5px;
  border-radius: 16px;
  border: solid 0px #f4d9e1;
}
#workspaces button {
  padding: 0px 10px;
  font-size: 19px;
  border-radius: 16px;
  /* color: black; */
  background-color: transparent;
  text-shadow: 0px 0px 3px rgba(0, 0, 0, 1);
}

#workspaces button.active {
  color: #f4d9e1;
  background-color: transparent;
  border-radius: 16px;
}

#workspaces button:hover {
  background-color: #e6b9c6;
  color: black;
  border-radius: 16px;
}

#custom-date,
#clock,
#battery,
#pulseaudio,
#network,
/* #custom-cava-internal, */

#custom-randwall,
#custom-launcher {
  background-color: #241717;
  background: transparent;
  padding: 5px 5px 5px 5px;
  margin: 5px 5px 5px 5px;
  border-radius: 8px;
  border: solid 0px #f4d9e1;
  color: #8bd5ca;
}
#cava {
  margin-right: 20px;
  color: pink;
}
